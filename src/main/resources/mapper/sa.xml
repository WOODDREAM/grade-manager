<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dfire.grade.manager.mapper.SaMapper">
    <resultMap id="saMap" type="com.dfire.grade.manager.bean.SaBean">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="queryById" resultMap="saMap" parameterType="String">
        select id,name
        from sa
        WHERE id=#{id}
        group by id
    </select>

    <insert id="insert" parameterType="com.dfire.grade.manager.bean.SaBean">
        INSERT INTO sa (id,name)
        VALUES (
        #{id},
        #{name}
        )
    </insert>
</mapper>