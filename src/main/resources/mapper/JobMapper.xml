<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dfire.grade.manager.mapper.JobMapper">
    <resultMap id="jobMapper" type="com.dfire.grade.manager.bean.Job">
        <id column="job_id" property="jobId" jdbcType=""/>
        <result column="name" property="name" jdbcType=""/>
        <result column="detail" property="detail" jdbcType=""/>
        <result column="tea_id" property="teacherId" jdbcType=""/>
        <result column="cla_id" property="classId" jdbcType=""/>
        <result column="answer" property="answer" jdbcType=""/>
        <result column="create_time" property="createTime" jdbcType=""/>
        <result column="type" property="type" jdbcType=""/>
        <result column="valid" property="valid" jdbcType=""/>
    </resultMap>
    <insert id="insertJob" parameterType="com.dfire.grade.manager.bean.Job">
        INSERT INTO job(job_id,name,datail,tea_id,cla_id,answer,create_time,type,valid)
        VALUES (
        #{jobId},
        #{name},
        #{detail},
        #{teacherId},
        #{classId},
        #{answer},
        #{createTime},
        #{type},
        #{valid}
        )
    </insert>
</mapper>